        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "ops"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 88}
      },
      {
        "id": 30,
        "title": "API Health Score",
        "type": "stat",
        "targets": [
          {
            "expr": "(1 - (sum(rate(inventory_api_errors_total[5m])) / sum(rate(inventory_api_requests_total[5m])))) * 100",
            "legendFormat": "Health Score",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 95},
                {"color": "green", "value": 99}
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "horizontal",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 96}
      },
      {
        "id": 31,
        "title": "Average Response Time",
        "type": "stat",
