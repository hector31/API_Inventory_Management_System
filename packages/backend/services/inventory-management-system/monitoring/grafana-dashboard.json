      },
      {
        "id": 29,
        "title": "Product Updates vs Queries",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(inventory_api_requests_total{method=\"GET\",endpoint=~\".*inventory.*\"}[5m]))",
            "legendFormat": "Read Operations",
            "refId": "A"
          },
          {
            "expr": "sum(rate(inventory_api_requests_total{method=~\"POST|PUT|PATCH|DELETE\",endpoint=~\".*inventory.*\"}[5m]))",
