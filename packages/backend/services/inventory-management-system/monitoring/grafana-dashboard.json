{
  "dashboard": {
    "id": null,
    "title": "Inventory Management API Metrics",
    "tags": ["inventory", "api", "opentelemetry"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "API Request Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(inventory_api_requests_total[5m]))",
            "legendFormat": "Requests/sec",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 50}
              ]
            },
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "API Error Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(inventory_api_errors_total[5m])) / sum(rate(inventory_api_requests_total[5m])) * 100",
            "legendFormat": "Error Rate %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            },
            "unit": "percent"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "API Response Time (95th percentile)",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(inventory_api_request_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "95th percentile",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.5},
                {"color": "red", "value": 1}
              ]
            },
            "unit": "s"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Active Endpoints",
        "type": "stat",
        "targets": [
          {
            "expr": "count(count by (endpoint)(inventory_api_requests_total))",
            "legendFormat": "Endpoints",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "value"
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "Request Rate by Endpoint",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(inventory_api_requests_total[5m])) by (endpoint)",
            "legendFormat": "{{endpoint}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 6,
        "title": "Response Time by Endpoint",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(inventory_api_request_duration_seconds_bucket[5m])) by (le, endpoint))",
            "legendFormat": "{{endpoint}} (95th)",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, sum(rate(inventory_api_request_duration_seconds_bucket[5m])) by (le, endpoint))",
            "legendFormat": "{{endpoint}} (50th)",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "s"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 7,
        "title": "HTTP Status Codes",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum(rate(inventory_api_requests_total[5m])) by (status_code)",
            "legendFormat": "{{status_code}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 16}
      },
      {
        "id": 8,
        "title": "Inventory Operations by Store",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(inventory_updates_total[5m])) by (store_id)",
            "legendFormat": "{{store_id}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "ops"
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 16}
      },
      {
        "id": 9,
        "title": "Product Queries by Operation",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(inventory_products_queried_total[5m])) by (operation)",
            "legendFormat": "{{operation}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "ops"
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 16}
      },
      {
        "id": 10,
        "title": "Requests by Client IP Type",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(inventory_api_requests_total[5m])) by (client_ip_type)",
            "legendFormat": "{{client_ip_type}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "reqps",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "single",
            "sort": "none"
          },
          "legend": {
            "displayMode": "visible",
            "placement": "bottom"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 11,
        "title": "Client IP Type Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum(rate(inventory_api_requests_total[5m])) by (client_ip_type)",
            "legendFormat": "{{client_ip_type}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "reqps",
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "pieType": "pie",
          "tooltip": {
            "mode": "single",
            "sort": "none"
          },
          "legend": {
            "displayMode": "visible",
            "placement": "right"
          },
          "displayLabels": ["name", "percent"]
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      },
      {
        "id": 12,
        "title": "Error Rate by Client IP Type",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(inventory_api_errors_total[5m])) by (client_ip_type) / sum(rate(inventory_api_requests_total[5m])) by (client_ip_type) * 100",
            "legendFormat": "{{client_ip_type}} Error Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "percent",
            "min": 0,
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32}
      },
      {
        "id": 13,
        "title": "Response Time by Client IP Type",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(inventory_api_request_duration_seconds_bucket[5m])) by (le, client_ip_type))",
            "legendFormat": "{{client_ip_type}} (95th)",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, sum(rate(inventory_api_request_duration_seconds_bucket[5m])) by (le, client_ip_type))",
            "legendFormat": "{{client_ip_type}} (50th)",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "s"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32}
      },
      {
        "id": 14,
        "title": "Rate Limiting Violations",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(inventory_api_errors_total{status_code=\"429\"}[5m]))",
            "legendFormat": "Rate Limit Hits",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 10}
              ]
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 40}
      },
      {
        "id": 15,
        "title": "Rate Limiting by IP Type",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(inventory_api_errors_total{status_code=\"429\"}[5m])) by (client_ip_type)",
            "legendFormat": "{{client_ip_type}} Rate Limited",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 8, "w": 18, "x": 6, "y": 40}
      },
      {
        "id": 16,
        "title": "API Methods Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum(rate(inventory_api_requests_total[5m])) by (method)",
            "legendFormat": "{{method}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "reqps"
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "pieType": "pie",
          "tooltip": {
            "mode": "single"
          },
          "legend": {
            "displayMode": "visible",
            "placement": "right"
          },
          "displayLabels": ["name", "percent"]
        },
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 48}
      },
      {
        "id": 17,
        "title": "Admin vs Regular Endpoints",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(inventory_api_requests_total{endpoint=~\"/v1/admin.*\"}[5m]))",
            "legendFormat": "Admin Endpoints",
            "refId": "A"
          },
          {
            "expr": "sum(rate(inventory_api_requests_total{endpoint!~\"/v1/admin.*\"}[5m]))",
            "legendFormat": "Regular Endpoints",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 48}
      },
      {
        "id": 18,
        "title": "Event Processing Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(inventory_events_processed_total[5m]))",
            "legendFormat": "Events Processed/sec",
            "refId": "A"
          },
          {
            "expr": "sum(rate(inventory_events_published_total[5m]))",
            "legendFormat": "Events Published/sec",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "ops"
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 48}
      },
      {
        "id": 19,
        "title": "Top Endpoints by Request Volume",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, sum(rate(inventory_api_requests_total[5m])) by (endpoint))",
            "legendFormat": "{{endpoint}}",
            "refId": "A",
            "format": "table",
            "instant": true
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 10}
              ]
            },
            "unit": "reqps"
          }
        },
        "options": {
          "showHeader": true
        },
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true,
                "__name__": true,
                "job": true,
                "instance": true
              },
              "indexByName": {},
              "renameByName": {
                "endpoint": "Endpoint",
                "Value": "Requests/sec"
              }
            }
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 56}
      },
      {
        "id": 20,
        "title": "Error Breakdown by Status Code",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(inventory_api_errors_total[5m])) by (status_code)",
            "legendFormat": "HTTP {{status_code}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 56}
      },
      {
